# .bash_profile - Managed with Ansible - DO NOT EDIT

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
    . ~/.bashrc
fi

# User specific environment and startup programs
if ! [[ "$EDITOR" =~ "/usr/bin/vi" ]]; then
    EDITOR=/usr/bin/vi
fi
export EDITOR

if ! [[ "$PS1" =~ "\u@local:\w\\$ " ]]; then
    PS1="\u@local:\w\\$ "
fi
export PS1
